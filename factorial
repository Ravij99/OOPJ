import java.util.Scanner;
public class Main

{
	public static void main(String[] args) 
	{
	    
	Scanner s1=new Scanner(System.in);
	int n,i,fact=1;
	System.out.println("enter the ppositive nummber n ");
	n=s1.nextInt();
	for(i=1;i<=n;i++)
	{
	    fact=fact*i;
	}
	System.out.println("factorial is = "+fact);

  }
}
